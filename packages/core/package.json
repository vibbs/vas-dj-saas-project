{
  "name": "@vas-dj-saas/core",
  "version": "0.1.0",
  "private": true,
  "description": "Core business logic and shared utilities for VAS-DJ SaaS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
