    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {},
  "peerDependencies": {
    "@vas-dj-saas/api-client": "workspace:*",
    "react": "19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@vas-dj-saas/api-client": "workspace:*",
    "react": "19.0.0",
    "typescript": "5.8.3"
  }
}
