  /api/v1/auth/logout/:
    post:
      operationId: v1_auth_logout_create
      description: Logout user by blacklisting the provided refresh token. This prevents
        the refresh token from being used again.
      summary: User Logout
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/LogoutRequestRequest'
