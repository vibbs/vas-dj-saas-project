components:
  schemas:
    Account:
      type: object
      description: Serializer for Account model with read-only computed fields.
      properties:
        id:
          type: string
          format: uuid
          readOnly: true
        email:
          type: string
          format: email
          maxLength: 254
        firstName:
          type: string
          maxLength: 30
        lastName:
          type: string
          maxLength: 30
        fullName:
          type: string
          readOnly: true
        abbreviatedName:
          type: string
          readOnly: true
        avatar:
          type: string
          format: uri
          nullable: true
          maxLength: 200
        phone:
          type: string
          maxLength: 20
        bio:
          type: string
        dateOfBirth:
          type: string
          format: date
          nullable: true
        gender:
          enum:
          - MALE
          - FEMALE
          - OTHER
          - UNKNOWN
          type: string
          description: |-
            * `MALE` - MALE
            * `FEMALE` - FEMALE
            * `OTHER` - OTHER
            * `UNKNOWN` - UNKNOWN
          x-spec-enum-id: 1486b1341af22a82
        role:
          enum:
          - ADMIN
          - USER
          - guest
          type: string
          description: |-
            * `ADMIN` - ADMIN
            * `USER` - USER
            * `guest` - guest
          x-spec-enum-id: bc9d89b95c849197
        isActive:
          type: boolean
          readOnly: true
        isEmailVerified:
          type: boolean
        isPhoneVerified:
          type: boolean
        is2faEnabled:
          type: boolean
        dateJoined:
          type: string
          format: date-time
          readOnly: true
        status:
          enum:
          - ACTIVE
          - INACTIVE
          - SUSPENDED
          - DELETED
          - BANNED
          - PENDING
          type: string
          description: |-
            * `ACTIVE` - ACTIVE
            * `INACTIVE` - INACTIVE
            * `SUSPENDED` - SUSPENDED
            * `DELETED` - DELETED
            * `BANNED` - BANNED
            * `PENDING` - PENDING
          x-spec-enum-id: 3d713a1dcd5ca6a3
        isAdmin:
          type: boolean
          readOnly: true
        isOrgAdmin:
          type: boolean
        isOrgCreator:
          type: boolean
        canInviteUsers:
          type: boolean
        canManageBilling:
          type: boolean
        canDeleteOrg:
          type: boolean
      required:
      - abbreviatedName
      - dateJoined
      - email
      - fullName
      - id
      - isActive
      - isAdmin
    AccountAuthProvider:
      type: object
      description: Serializer for authentication provider information.
      properties:
        id:
          type: string
          format: uuid
          readOnly: true
        provider:
          type: string
          maxLength: 50
        providerUserId:
          type: string
          maxLength: 255
        email:
          type: string
          format: email
          maxLength: 254
        linkedAt:
          type: string
          format: date-time
          readOnly: true
        isPrimary:
          type: boolean
      required:
      - email
      - id
      - linkedAt
      - provider
      - providerUserId
    AccountCreateRequest:
      type: object
      description: Serializer for creating new user accounts with password confirmation.
      properties:
        email:
          type: string
          format: email
          minLength: 1
          maxLength: 254
        password:
          type: string
          writeOnly: true
          minLength: 8
        passwordConfirm:
          type: string
          writeOnly: true
          minLength: 1
        firstName:
          type: string
          maxLength: 30
        lastName:
          type: string
          maxLength: 30
        phone:
          type: string
          maxLength: 20
        bio:
          type: string
        dateOfBirth:
          type: string
          format: date
          nullable: true
        gender:
          enum:
